@namespace BlazorUI.Primitives.Collapsible

<div role="region"
     data-state="@(Context?.Open ?? false ? "open" : "closed")"
     aria-hidden="@(!(Context?.Open ?? false))"
     style="@(Context?.Open ?? false ? "" : "display: none;")"
     @attributes="AdditionalAttributes">
    @ChildContent
</div>
